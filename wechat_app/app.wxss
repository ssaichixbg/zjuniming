/**app.wxss**/
.container {
  width:100%;
  height: 100%;
  position: absolute;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  box-sizing: border-box;
  font-family: PingFangSC-Regular, 'Franklin Gothic Medium', 'Arial Narrow', Arial, sans-serif;
  overflow: hidden;
} 

/***title***/
.title-background{
   background-color: #23d5b1;
   width:100%;
   display: flex;
   flex-direction: row;
   justify-content: center;
   align-items: center;
}
.title-text{
  font-size:37rpx;
  color:#ffffff;
  letter-spacing:0;
  line-height: 70rpx;
  font-weight: bold;
}